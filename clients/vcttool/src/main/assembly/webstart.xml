<assembly>
	<id>start</id>
	<formats>
		<format>dir</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>

	<dependencySets>
		<dependencySet>
			<outputDirectory>libs</outputDirectory>
			<excludes>
				<exclude>*:${artifactId}</exclude>
			</excludes>
		</dependencySet>
	</dependencySets>

	<files>
		<file>
			<source>target/${artifactId}-${version}.jar</source>
		</file>
	</files>
</assembly>
